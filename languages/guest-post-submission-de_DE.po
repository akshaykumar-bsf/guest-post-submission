#. Plugin Name of the plugin
msgid "Guest Post Submission"
msgstr "Gastbeitrag Einreichung"

#. Plugin URI of the plugin
msgid "https://yourwebsite.com/guest-post-submission"
msgstr "https://yourwebsite.com/guest-post-submission"

#. Description of the plugin
msgid "Allows visitors to submit guest posts through a front-end form."
msgstr "Ermöglicht Besuchern, Gastbeiträge über ein Frontend-Formular einzureichen."

#. Author of the plugin
msgid "Your Name"
msgstr "Dein Name"

#. Author URI of the plugin
msgid "https://yourwebsite.com"
msgstr "https://yourwebsite.com"

#: public/class-shortcode.php:45
msgid "You have reached the submission limit. Please try again later."
msgstr "Sie haben das Einreichungslimit erreicht. Bitte versuchen Sie es später erneut."

#: templates/submission-form.php:3
msgid "Submit a Guest Post"
msgstr "Einen Gastbeitrag einreichen"

#: templates/submission-form.php:8
msgid "Post Title"
msgstr "Beitragstitel"

#: templates/submission-form.php:10
msgid "Please enter a post title."
msgstr "Bitte geben Sie einen Beitragstitel ein."

#: templates/submission-form.php:15
msgid "Post Content"
msgstr "Beitragsinhalt"

#: templates/submission-form.php:41
msgid "Please enter post content (minimum 100 characters)."
msgstr "Bitte geben Sie den Beitragsinhalt ein (mindestens 100 Zeichen)."

#: templates/submission-form.php:46
msgid "Your Name"
msgstr "Ihr Name"

#: templates/submission-form.php:48
msgid "Please enter your name."
msgstr "Bitte geben Sie Ihren Namen ein."

#: templates/submission-form.php:53
msgid "Your Email"
msgstr "Ihre E-Mail"

#: templates/submission-form.php:55
msgid "Please enter a valid email address."
msgstr "Bitte geben Sie eine gültige E-Mail-Adresse ein."

#: templates/submission-form.php:61
msgid "About You"
msgstr "Über Sie"

#: templates/submission-form.php:63
msgid "Please tell us a bit about yourself."
msgstr "Bitte erzählen Sie uns etwas über sich."

#: templates/submission-form.php:68
msgid "Featured Image"
msgstr "Beitragsbild"

#: templates/submission-form.php:70
msgid "Maximum file size: 2MB. Recommended dimensions: 1200x628 pixels."
msgstr "Maximale Dateigröße: 2MB. Empfohlene Abmessungen: 1200x628 Pixel."

#: templates/submission-form.php:78
msgid "Submit Post"
msgstr "Beitrag einreichen"

#: templates/submission-form.php:86
msgid "Thanks for your submission! We'll review and get back soon."
msgstr "Vielen Dank für Ihre Einreichung! Wir werden sie überprüfen und uns bald bei Ihnen melden."

#: templates/submission-form.php:90
msgid "There was an error submitting your post. Please try again."
msgstr "Beim Einreichen Ihres Beitrags ist ein Fehler aufgetreten. Bitte versuchen Sie es erneut."

#: templates/submission-form.php:94
msgid "Loading..."
msgstr "Wird geladen..."

#: templates/submission-form.php:96
msgid "Submitting your post..."
msgstr "Ihr Beitrag wird eingereicht..."

#: includes/class-form-handler.php:11
msgid "Security check failed"
msgstr "Sicherheitsüberprüfung fehlgeschlagen"

#: includes/class-form-handler.php:33
msgid "Your post has been submitted successfully and is awaiting review."
msgstr "Ihr Beitrag wurde erfolgreich eingereicht und wartet auf Überprüfung."

#: includes/class-submission-validator.php:25
msgid "Post title is required."
msgstr "Beitragstitel ist erforderlich."

#: includes/class-submission-validator.php:29
msgid "Post content is required."
msgstr "Beitragsinhalt ist erforderlich."

#: includes/class-submission-validator.php:33
msgid "Post content must be at least 100 characters."
msgstr "Der Beitragsinhalt muss mindestens 100 Zeichen umfassen."

#: includes/class-submission-validator.php:37
msgid "Author name is required."
msgstr "Autorname ist erforderlich."

#: includes/class-submission-validator.php:41
msgid "Author email is required."
msgstr "Autoren-E-Mail ist erforderlich."

#: includes/class-submission-validator.php:45
msgid "Please enter a valid email address."
msgstr "Bitte geben Sie eine gültige E-Mail-Adresse ein."

#: includes/class-submission-validator.php:49
msgid "Author bio is required."
msgstr "Autorenbiografie ist erforderlich."

#: includes/class-submission-validator.php:55
msgid "Featured image must be a valid image file (jpg, jpeg, png, gif)."
msgstr "Das Beitragsbild muss eine gültige Bilddatei sein (jpg, jpeg, png, gif)."

#: includes/class-submission-validator.php:59
msgid "Featured image size must be less than 2MB."
msgstr "Die Größe des Beitragsbilds muss weniger als 2MB betragen."

